import discord
import time
from discord.ext import commands
import random as r
from bs4 import BeautifulSoup
from urllib.request import urlopen
import requests
import string



bot = commands.Bot(command_prefix="!", intents=discord.Intents().all())

BOT_TOKEN = ""

@bot.event
async def on_ready():
    print("------------\n")
    print("i Joined the server")

@bot.command()
async def hello(ctx):
    await ctx.send("HELLO!!!!!!!!")

@bot.command()
async def joke(ctx):
    await ctx.send("whats 9 + 10?")
    time.sleep(5)
    await ctx.send("21!")

@bot.command()
async def whatsthat(ctx):
    time.sleep(1)
    await ctx.send("GIRL THATS A BOOTY HOLE!")

@bot.command()
async def rimage(ctx):
    while True:
        urlImgurStringLength = 5
        temp = ''.join([r.choice(
                            string.ascii_letters)
                            for n in range(urlImgurStringLength)])
        finURL = ("https://imgur.com/" + temp)
        lengthTest = len(requests.post(finURL).content)
        if lengthTest != 4323:
            await ctx.send(finURL)
            break

@bot.command()
async def misato(ctx):
    await ctx.send("Under Construction")


bot.run(BOT_TOKEN)
